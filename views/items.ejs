  <% include ../partials/header %>
   <body class="w3-black">
    <div class="w3-main" style="margin-left:340px;margin-right:40px">
  <div class="row">
    <% items.forEach(function(items) { %>
                
                <div class="item">
                    <%= items.id %><br>
                    <%= items.name %><br>
                    <%= items.email %><br>
                    <%= items.Comment %><br>
                    <%= items.price %><br>
                <img src="/<%= items.photo%>" style="width:100%"><br>
                 <a href="/deleteitems/<%= items.id %>" class="button">Delete</a><br>
              
                   
                    </div>
              
         <% }); %>
  </div>


<% include ../partials/footer %>