  <% include ../partials/header %>
   <body class="w3-black">
<div class="w3-main" style="margin-left:340px;margin-right:40px">
<div class="row">
  <h1>Welcome to the add jobs page  <br>
  <div class="grid-container">
 <% indOne.forEach(function(contacts) { %>        
            <form accept-charset="UTF-8" action="/editjobs/<%= contacts.id %>" method="post">
   
                <div class="item">
                  <label for="name"><%= contacts.name %></label>
                    <input name="name" type="text" value='<%= contacts.name %>' />
                  
                  
                  <label for="price">Comment</label>
                    <input name="comment" type="text" value='<%= contacts.Comment %>' />
                    
                   
                   <label for="image">Email</label> 
                    <input name="email" type="text" value='<%= contacts.email %>' />
                    
                   
                    <button class="button">Edit</button>
                   
                   
                    </div>
              </form>
      <% }); %> 
         
         </body>
    </div>
